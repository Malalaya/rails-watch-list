<div class= container>
  <h1>My Movie lists</h1>
</div>

<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(https://cdn.pixabay.com/photo/2017/09/25/13/12/puppy-2785074_1280.jpg);">
  <div class= container>
    <ul>
      <% @lists.each do |list| %>
        <p> <li><%= link_to list.name, list_path(list) %></li> </p>
      <% end %>
    </ul>
  </div>
</div>
